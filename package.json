{
  "name": "ferrytempo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "docs": "mkdir -p docs && jsonschema2md -o docs/ -d schemas/ -x -"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@adobe/jsonschema2md": "^7.0.0",
    "ajv": "^8.11.0",
    "node-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "chai": "^4.3.6",
    "chai-json-schema": "^1.5.1",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.2.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true,
      "mocha": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "browser": true,
      "node": true,
      "es2020": true,
      "requireConfigFile": false
    },
    "rules": {
      "max-len": [
        2,
        120,
        4,
        {
          "ignoreUrls": true
        }
      ],
      "no-console": 0,
      "object-curly-spacing": 0
    }
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
